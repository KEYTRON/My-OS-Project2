/* kernel.ld */
OUTPUT_FORMAT(elf32-i386)
ENTRY(start)        /* Указываем начальную точку входа */
SECTIONS
{
    . = 0x00100000;  /* Адрес загрузки ядра в память */
    .text : { *(.text) }  /* Секция с кодом */
    .data : { *(.data) }  /* Секция с данными */
    .bss  : { *(.bss) }   /* Нулевые данные */
}